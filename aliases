# General aliases
alias c='cd ~/Dev'
alias n='cd ~/notes'
alias tmp='cd ~/tmp'
alias vimrc="vim ~/.vimrc"
alias cronjobs='cd ~/cronjobs/'
#alias tmux='tmux -u'
alias rsyncgit="rsync -avzh --filter=':- .gitignore'"
alias rsync="rsync -avz --progress -h"
alias zshconfig="vim ~/.zshrc"

